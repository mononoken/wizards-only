<h1>New Post</h1>

<%= render 'user_bar' %>

<%= form_with model: @post do |form| %>
  <ul>
    <li>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </li>
    <li>
      <%= form.label :body %>
      <%= form.text_area :body %>
    </li>
    <li>
      <%= form.submit %>
    </li>
  </ul>
<% end %>
